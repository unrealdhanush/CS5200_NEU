storeObjs <- function(folder,root,verbose=T)
{
files <- list.files(path = folder, all.files = F, full.names = F)
filenames <- as.list(files)
if(verbose) {
print("Copying ",getFileName(filenames[1])," to ",getTags(filenames[1]))
}
for (i in 1:length(filenames)) {
Fname <- list()
Tname <- list()
Fnames <- append(Fname, getFileName(filenames[i]))
filepath <- paste("/Users/dhanush/Northeastern/2023summer/CS5200/CS5200.BuildDocDB.balakrishna/files", Fnames[1], sep = "/")
Tags <- getTags(filenames[i], rettype = "list")
for (k in 1:length(Tags)) {
Tnames <- append(Tname, Tags[k])
Tname <- Tnames
}
for (j in 1:length(Tnames)) {
dir_root <- paste(configDB(mode="path",root),getObjPath(root,Tnames[j]),sep="/")
temp_root <- paste(dir_root,"/")
dir.create(dir_root, recursive=F)
file.copy(filepath, temp_root)
}
}
}
